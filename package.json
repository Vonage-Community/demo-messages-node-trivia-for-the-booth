{
  "name": "@vonage/demo-messages-node-trivia-for-the-booth",
  "version": "1.0.0",
  "description": "A simple trivia game to be run at Vonages booth ",
  "license": "Apache-2.0",
  "author": "",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently 'npm run dev:web' 'npm run dev:api'",
    "dev:api": "node --watch server/index.js",
    "dev:web": "cross-env SASS_SILENCE_ALL=all vite",
    "eslint": "eslint",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "preview": "vite preview --host --port 5173",
    "test": "cross-env NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--experimental-vm-modules\" jest",
    "test:cov": "cross-env NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--experimental-vm-modules\" jest --coverage",
    "test:watch": "cross-env NODE_NO_WARNINGS=1 NODE_OPTIONS=\"--experimental-vm-modules\" jest --watch"
  },
  "dependencies": {
    "@faker-js/faker": "10.0.0",
    "@popperjs/core": "2.11.8",
    "@vonage/jwt": "1.12.1",
    "bcryptjs": "3.0.2",
    "better-sqlite3": "12.3.0",
    "body-parser": "2.2.0",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.13.1",
    "debug": "4.4.3",
    "dotenv": "17.2.3",
    "eslint-plugin-jest": "29.0.1",
    "express": "5.1.0",
    "handlebars": "4.7.8",
    "jayson": "4.2.0",
    "jsonwebtoken": "9.0.2",
    "openai": "6.4.0"
  },
  "devDependencies": {
    "@eslint/js": "9.36.0",
    "@stylistic/eslint-plugin": "5.4.0",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "eslint-plugin-html": "8.1.3",
    "eslint-plugin-n": "17.23.1",
    "jest": "30.2.0",
    "sass": "1.93.2",
    "supertest": "7.1.4",
    "vite": "7.1.7",
    "vite-plugin-handlebars": "2.0.0"
  }
}
